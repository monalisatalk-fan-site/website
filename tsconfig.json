{
  "compilerOptions": {
    "lib": ["dom", "esnext"],
    "target": "es6",
    "module": "esnext",
    "moduleResolution": "node",
    "jsx": "preserve",
    "allowJs": true,
    "isolatedModules": true,
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "noEmit": true,
    "baseUrl": ".",
    "types": [
      "node",
      "@nuxt/types",
      "@nuxtjs/firebase",
      "@nuxtjs/dayjs",
      "@lollipop-onl/vue-typed-reactive"
    ]
  }
}
